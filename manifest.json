{
  "name": "Auto Reload Page",
  "version": "1.1",
  "icons": {
    "128": "icon.png"
  },
  "description": "Keep reloading page automatically!",
  "permissions": ["storage", "tabs"],
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },
  "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
  "manifest_version": 2
}